<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.surgical.decision3">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">

        <activity
            android:name="com.surgical.decision3.panel.MainActivity"
            android:label="@string/app_name"
            android:launchMode="singleInstance"
            android:alwaysRetainTaskState="true">


            <!-- List of Components to use -->
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>

            <!-- Make sure of Singleton on instance -->

        </activity>

        <activity android:label="Intervention Activity" android:name="com.surgical.decision3.panel.InterventionActivity"/>

    </application>

    <uses-permission android:name="ANDROID.PERMISSION.READ_EXTERNAL_STORAGE"/>
    <uses-permission android:name="ANDROID.PERMISSION.WRITE_EXTERNAL_STORAGE"/>

</manifest>