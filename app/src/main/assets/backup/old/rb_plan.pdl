(define (plan rb_plan)
	(:domain rb)
	(:problem pb1)
   	(:planvars )
	(:init )
	(:body
		(sequence
			(action (insert_rubber_dam) 
				:desired_outcome (AND 
						( CLAMP_WITH_RB_ON_TOOTH )
